<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<context:annotation-config></context:annotation-config>

<bean id="myProfile" class="com.qualifier.Profile">
</bean>

<bean id="Student1" class="com.qualifier.Student">
<property name="id" value="1"></property>
<property name="name" value="Sush"></property>
</bean>

<bean id="Student2" class="com.qualifier.Student">
<property name="id" value="2"></property>
<property name="name" value="ABC"></property>
</bean>

<bean id="Student3" class="com.qualifier.Student">
<property name="id" value="3"></property>
<property name="name" value="DEF"></property>
</bean>


	
    

</beans>