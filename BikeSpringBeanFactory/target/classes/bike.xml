<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<bean id="myBike" class="com.Bike">
<property name="brand" value="Pulsar"></property>
<property name="color" value="Black"></property>
<property name="cost" value="30000.00"></property>
</bean>

<bean id="myBike1" class="com.Bike">
<property name="brand" value="KTM"></property>
<property name="color" value="Orange"></property>
<property name="cost" value="80000.00"></property>
</bean>
    

</beans>